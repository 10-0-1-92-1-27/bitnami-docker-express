{
  "v": "4.0",
  "workspace": {
    "name": "express",
    "defaultEnv": "express",
    "environments": [
      {
        "name": "express",
        "recipe": null,
        "machineConfigs": [
          {
            "name": "ws-machine",
            "limits": {
              "ram": 1000
            },
            "source": {
              "type": "dockerfile",
              "location": "https://raw.githubusercontent.com/amrecio/bitnami-docker-express/master/.codenvy.dockerfile"
            },
            "type": "docker",
            "dev": true
          }
        ]
      }
    ],
    "projects": [
      {
        "name": "express-sample",
        "type": "node-js",
        "path": "/express-sample",
        "source": {
        },
        "mixins": [
          "git"
        ],
        "modules": [],
        "problems": []
      }
    ],
    "commands": [
      {
        "name": "create sample project",
        "type": "custom",
        "commandLine": "cd ${current.project.path} && express . -f",
        "attributes": {
          "previewUrl": ""
        }
      }
    ]
  },
  "ide": {
    "onProjectsLoaded": {
      "actions": [
        {
          "properties": {
            "file": "/express-sample/README.md"

          },
          "id": "openFile"

        },
        {
          "properties": {
            "name": "create sample project"

          },
          "id": "runCommand"

        }

      ]

    }

  }
}
